<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.DVVARDHAN.educarepoc">

    <uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Results"></activity>
        <activity android:name=".Attendance" />
        <activity android:name=".Timetable" />
        <activity android:name=".Dairy" />
        <activity android:name=".Profile" />
        <activity android:name=".HomePage" />
        <activity android:name=".UserInfoExpandable" />
        <activity
            android:name=".Adduser"
            android:theme="@style/Noactionbar"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".Login"
            android:theme="@style/Login"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Login" />
    </application>

</manifest>